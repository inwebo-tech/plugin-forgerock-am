<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ServicesConfiguration
        PUBLIC "=//iPlanet//Service Management Services (SMS) 1.0 DTD//EN"
        "jar://com/sun/identity/sm/sms.dtd">
<ServicesConfiguration>
    <Service name="iPlanetAMAuthInWeboPushAuthService" version="1.0">
        <Schema serviceHierarchy="/DSAMEConfig/authentication/iPlanetAMAuthInWeboPushAuthService"
                i18nFileName="amAuthInWeboPushAuth"
                revisionNumber="10"
                i18nKey="inwebo.i18n.name"
                resourceName="inWeboPush">
            <Organization>
                <AttributeSchema name="iplanet-am-auth-inwebo-push-base-url"
                                 type="single"
                                 syntax="string"
                                 order="100"
                                 i18nKey="inwebo.i18n.1.base.url"
                                 resourceName="inWeboPushBaseUrl">
                    <DefaultValues>
                        <Value>https://api.myinwebo.com/FS</Value>
                    </DefaultValues>
                </AttributeSchema>
                <AttributeSchema name="iplanet-am-auth-inwebo-push-service-id"
                                 type="single"
                                 syntax="number_range"
                                 rangeStart="2"
                                 rangeEnd="2147483647"
                                 order="200"
                                 i18nKey="inwebo.i18n.2.service.id"
                                 resourceName="inWeboPushServiceId">
                </AttributeSchema>
                <AttributeSchema name="iplanet-am-auth-inwebo-push-certificate-path"
                                 type="single"
                                 syntax="string"
                                 order="300"
                                 i18nKey="inwebo.i18n.3.certificate.path"
                                 resourceName="inWeboPushCertificatePath">
                    <DefaultValues>
                        <Value>/the/path/to/your/certificate.p12</Value>
                    </DefaultValues>
                </AttributeSchema>
                <AttributeSchema name="iplanet-am-auth-inwebo-push-certificate-password"
                                 type="single"
                                 syntax="password"
                                 order="400"
                                 i18nKey="inwebo.i18n.4.certificate.password"
                                 resourceName="inWeboPushCertificatePassword"/>
                <AttributeSchema name="iplanet-am-auth-inwebo-proxy-url"
                                 type="single"
                                 syntax="string"
                                 order="500"
                                 i18nKey="inwebo.i18n.5.proxy.url"
                                 resourceName="inWeboPushProxyUrl"/>
                <AttributeSchema name="iplanet-am-auth-inwebo-push-proxy-username"
                                 type="single"
                                 syntax="string"
                                 order="600"
                                 i18nKey="inwebo.i18n.6.proxy.username"
                                 resourceName="inWeboPushProxyUsername"/>
                <AttributeSchema name="iplanet-am-auth-inwebo-push-proxy-password"
                                 type="single"
                                 syntax="password"
                                 order="700"
                                 i18nKey="inwebo.i18n.7.proxy.password"
                                 resourceName="inWeboPushProxyPassword"/>
                <AttributeSchema name="iplanet-am-auth-inwebo-push-auth-level"
                                 type="single"
                                 syntax="number_range"
                                 rangeStart="0"
                                 rangeEnd="2147483647"
                                 i18nKey="a500"
                                 order="800"
                                 resourceName="authenticationLevel">
                    <DefaultValues>
                        <Value>1</Value>
                    </DefaultValues>
                </AttributeSchema>
                <SubSchema name="serverconfig" inheritance="multiple" resourceName="USE-PARENT">
                    <AttributeSchema name="iplanet-am-auth-inwebo-push-base-url"
                                     type="single"
                                     syntax="string"
                                     order="100"
                                     i18nKey="inwebo.i18n.1.base.url"
                                     resourceName="inWeboPushBaseUrl">
                        <DefaultValues>
                            <Value>https://api.myinwebo.com/FS</Value>
                        </DefaultValues>
                    </AttributeSchema>
                    <AttributeSchema name="iplanet-am-auth-inwebo-push-service-id"
                                     type="single"
                                     syntax="number_range"
                                     rangeStart="2"
                                     rangeEnd="2147483647"
                                     order="200"
                                     i18nKey="inwebo.i18n.2.service.id"
                                     resourceName="inWeboPushServiceId">
                    </AttributeSchema>
                    <AttributeSchema name="iplanet-am-auth-inwebo-push-certificate-path"
                                     type="single"
                                     syntax="string"
                                     order="300"
                                     i18nKey="inwebo.i18n.3.certificate.path"
                                     resourceName="inWeboPushCertificatePath">
                        <DefaultValues>
                            <Value>/the/path/to/your/certificate.p12</Value>
                        </DefaultValues>
                    </AttributeSchema>
                    <AttributeSchema name="iplanet-am-auth-inwebo-push-certificate-password"
                                     type="single"
                                     syntax="password"
                                     order="400"
                                     i18nKey="inwebo.i18n.4.certificate.password"
                                     resourceName="inWeboPushCertificatePassword"/>
                    <AttributeSchema name="iplanet-am-auth-inwebo-push-proxy-url"
                                     type="single"
                                     syntax="string"
                                     order="500"
                                     i18nKey="inwebo.i18n.5.proxy.url"
                                     resourceName="inWeboPushProxyUrl"/>
                    <AttributeSchema name="iplanet-am-auth-inwebo-push-proxy-username"
                                     type="single"
                                     syntax="string"
                                     order="600"
                                     i18nKey="inwebo.i18n.6.proxy.username"
                                     resourceName="inWeboPushProxyUsername"/>
                    <AttributeSchema name="iplanet-am-auth-inwebo-push-proxy-password"
                                     type="single"
                                     syntax="password"
                                     order="700"
                                     i18nKey="inwebo.i18n.7.proxy.password"
                                     resourceName="inWeboPushProxyPassword"/>
                    <AttributeSchema name="iplanet-am-auth-inwebo-push-auth-level"
                                     type="single"
                                     syntax="number_range"
                                     rangeStart="0"
                                     rangeEnd="2147483647"
                                     i18nKey="a500"
                                     order="800"
                                     resourceName="authenticationLevel">
                        <DefaultValues>
                            <Value>1</Value>
                        </DefaultValues>
                    </AttributeSchema>
                </SubSchema>
            </Organization>
        </Schema>
    </Service>
</ServicesConfiguration>